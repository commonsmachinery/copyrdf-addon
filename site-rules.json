{
    "formatVersion": 1,
    "sites": [
        {
            "name": "Flickr",
            "urls": [
                "(www\\.)?flickr\\.com/photos/"
            ],

            "rules": {
                "source": ["oembed"],

                "oembed": {
                    "type": "json",
                    "endpoint": "https://www.flickr.com/services/oembed",
                    "map": {
                        "license_url": {
                            "property": "http://www.w3.org/1999/xhtml/vocab#license",
                            "type": "uri"
                        },
                        "web_page": {
                            "property": "http://purl.org/dc/elements/1.1/identifier",
                            "type": "uri"
                        }
                    }
                },

                "mainElement": [
                    "img.main-photo",
                    "div#allsizes-photo > img",
                    "img#liquid-photo",
                    "span.facade-of-protection + img.loaded"
                ],

                "mainOverlayElements": {
                    "img.main-photo": "div.photo-well-view a.navigate-target, div.photo-well-view div.facade-of-protection-neue",
                    "img#liquid-photo": "div#photo-drag-proxy",
                    "span.facade-of-protection + img.loaded": "span.facade-of-protection"
                },

                "rewriteMainSubject": [
                    "oembed:web_page"
                ]
            }
        },

        {
            "name": "deviantART",
            "urls": [
                "(.+)\\.deviantart\\.com/art/"
            ],

            "rules": {
                "source": ["oembed", "rdfa"],

                "oembed": {
                    "type": "json",
                    "endpoint": "http://backend.deviantart.com/oembed"
                },

                "mainElement": [
                    "img.dev-content-normal"
                ],

                "rewriteMainSubject": [
                    "urlregex:(^.*?:[\\/]*)(?:[a-zA-Z0-9][a-zA-Z0-9-]*?\\.)(.*)\\??.*"
                ]
            }
        }
    ]
}
